@Html.Partial("../Common/MasterSearch")

@section scripts{
    <script>
        c1.documentReady(function () {
        });

        // 検索（F2）
        var dlgMasterSearch = wijmo.Control.getControl('#dlgMasterSearch');
        c1.documentReady(function () {
            //CAMPUS-ID
            document.getElementById('btnSearch').addEventListener('click', function () {
                loadMasterData();

                dlgMasterSearch.show(true, function (sender) {
                    if (sender.dialogResult == 'wj-hide-ok') {
                        document.getElementById('Key_CampusId').setAttribute('value', masterSelected.code);
                        document.getElementById('Key_CampusName').setAttribute('value', masterSelected.name);
                    }
                });
            });
            document.getElementById('Key_CampusId').addEventListener('keydown', CampusIdKeyDownEvent);
            function CampusIdKeyDownEvent(e) {
                if (e.keyCode == 113) {
                    let target = document.getElementById('btnSearch');
                    let clickEvent = new Event('click', { "bubbles": true, "cancelable": true });
                    target.dispatchEvent(clickEvent);
                }
            }
        });
    </script>
}