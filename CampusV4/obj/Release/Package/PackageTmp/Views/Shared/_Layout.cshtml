<body class="nav-md">
    @*GreapCity FlexGrid 個別対応*@
    <style>
        .wj-cell {
            border-color: black;
            border: 0.5px solid #e6e6fa;
            transition: all 100ms;
        }

            .wj-cell.wj-header {
                background: #efffff;
                color: #007d7f;
                font-weight: bold;
                text-align: center;
            }
        .wj-control .wj-input-group .wj-form-control {
            background: #ffffff;
            border: 1px solid #ccc;
            height: 35px;
        }
        .wj-popup .wj-dialog-header {
            width: 100%;
            background: #efffff;
            color: #007d7f;
            border: 0.5px solid #e6e6fa;
            font-weight: bold;
            font-size: 120%;
            padding: 1em;
        }
    </style>
    <script>
        c1.documentReady(function () {
            var display = 100;

            $('#ickExpand').click(function () {
                if (display <= 150) {
                    display += 10;
                }
                $("body").css("zoom", display + "%");
            });

            $('#ickCompress').click(function () {
                if (display >= 50) {
                    display -= 10;
                }
                $("body").css("zoom", display + "%");
            });

            $('#ickNormal').click(function () {
                display = 100;
                $("body").css("zoom", "100%");
            });
        });
    </script>
